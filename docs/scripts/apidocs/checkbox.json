{
  "props": [{
    "name": "id",
    "type": "String",
    "default": "",
    "description": "A unique identifier for the checkbox"
  }, {
    "name": "name",
    "type": "String",
    "default": "",
    "description": "The name attribute of the <checkbox>"
  }, {
    "name": "label",
    "type": "String",
    "default": "",
    "description": "The label shown above the checkbox"
  }, {
    "name": "hideLabel",
    "type": "Boolean",
    "default": "false",
    "description": "Applies no label effect"
  }, {
    "name": "value",
    "type": "String, Number, Boolean",
    "default": "",
    "description": "The value attribute of the <checkbox>"
  }, {
    "name": "model",
    "type": "Array, String, Number, Boolean",
    "default": "false (required)",
    "description": "The model that the checkbox value syncs to",
    "required": true
  }, {
    "name": "effect",
    "type": "Boolean",
    "default": "false",
    "description": "Applies ripple click effect"
  }, {
    "name": "disabled",
    "type": "Boolean",
    "default": "false",
    "description": "Applies disabled display effect"
  }, {
    "name": "filled",
    "type": "Boolean",
    "default": "false",
    "description": "Applies filled checked effect"
  }],
  "slots": [{
    "name": "(default)",
    "description": "The default slot holds the checkbox label and can contain HTML"
  }],
  "events": [{
    "name": "change",
    "description": "ispatched when the checkbox is changed"
  }]
}
